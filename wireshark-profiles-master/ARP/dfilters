"Meraki Dashboard Control traffic" ((ip.dst_host contains "meraki.com") or ((ip.addr eq 64.62.142.12/32 or ip.addr eq 108.161.147.0/24 or ip.addr eq 199.231.78.0/24 or ip.addr eq 209.206.48.0/20) and (udp.port eq 7351 or udp.port eq 9350 or tcp.port eq 7734)))
"Meraki MAC" (eth.addr_resolved contains "CiscoMer" or eth.addr >= 0c:8d:db:00:00:00 or eth.addr >= 88:15:44:00:00:00 or eth.addr >=e0:55:3d:00:00:00 or eth.addr >= 00:18:0a:00:00:00 or eth.addr >= e0:cb:bc:00:00:00)
"Ethernet broadcast" (eth.addr eq ff:ff:ff:ff:ff:ff)
"Ethernet type 0x0806 (ARP)" (eth.type eq 0x0806)
"Ethernet address 00:00:5e:00:53:00" (eth.addr eq 00:00:5e:00:53:00)
"ARP sourced by" (arp.src.hw_mac eq aa:ff:ff:ff:ff:ff)
"ARP for IP" (arp.dst.proto_ipv4 eq 169.254.255.255)
"Gratuitous ARP" ((arp.isgratuitous eq 1) and (arp.opcode eq 1 or arp.opcode eq 2))
"Uplink duplicate IP search" (arp.src.proto_ipv4 eq 0.0.0.0)
"ARP for IP with certain mac" (arp.dst.proto_ipv4 eq 169.254.255.255) or (eth.addr eq aa:ff:ff:ff:ff:ff)
"Duplicate IP detected" (arp.duplicate-address-detected)
"ARP reply" (arp.opcode eq 2)
"ARP request" (arp.opcode eq 1)
"Only ARP" (arp or eth.type eq 0x0806)
